    "start": "nest start",
    "start:heroku": "npm run migrate:latest && npm run build && npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "NODE_ENV=test jest --forceExit",
    "test:watch": "NODE_ENV=test jest --watch --forceExit",
    "test:cov": "NODE_ENV=test jest --coverage --forceExit",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "migrate:create_db": "ts-node src/scripts/create_db.ts",
    "migrate:make": "knex migrate:make",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:latest": "knex migrate:latest"
  },
